              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT Patient.MedicalRecordNo,ReferralSource.OrganizationName AS ReferralSource,'Referral' AS DetailType FROM Patient.Patient
INNER JOIN Common.Person ReferralSource on ReferralSource.Id = Patient.ReferralSource_Id
WHERE Patient.Team_Id = 500043
AND CAST(ReferralDate AS DATE) = CAST(GETDATE() AS DATE)

UNION

SELECT   Patient.MedicalRecordNo,ReferralSource.OrganizationName AS ReferralSource,'Refill' AS DetailType FROM  Patient.Patient
INNER JOIN Common.Person ReferralSource on ReferralSource.Id = Patient.ReferralSource_Id
INNER JOIN Prescription.PrescRiption on Prescription.Patient_Id = Patient.Id
INNER JOIN Prescription.PrescriptionFill on PrescriptionFill.Prescription_Id = Prescription.Id
where  CAST(ActualCompoundingDate AS DATE) = CAST(GETDATE() AS DATE)
AND PrescriptionFill.Sequence != 1
AND Patient.Team_Id = 500043

UNION 

SELECT   Patient.MedicalRecordNo,ReferralSource.OrganizationName AS ReferralSource,'Order Made Ready to Fill' AS DetailType FROM  Patient.Patient 
INNER JOIN Common.Person ReferralSource on ReferralSource.Id = Patient.ReferralSource_Id
INNER JOIN Prescription.PrescRiption on Prescription.Patient_Id = Patient.Id
INNER JOIN Prescription.PrescriptionFill on PrescriptionFill.Prescription_Id = Prescription.Id
AND PrescriptionFill.Sequence != 1
where  CAST(PrescriptionFill.CreatedDate AS DATE) = CAST(GETDATE() AS DATE)
AND Patient.Team_Id = 500043
order by DetailType,ReferralSource</property>
