          SQLTask:SqlStatementSource="CREATE TABLE #EligiblePatients&#xA;(&#xA;DailyReferralCount BIGINT,&#xA;WeeklyReferralCount BIGINT,&#xA;MonthlyReferralCount BIGINT,&#xA;TotalReferralCount BIGINT,&#xA;DailyRefillCount BIGINT,&#xA;DailyOrderMadeReadyToFillCount BIGINT,&#xA;WeeklyRefillCount BIGINT,&#xA;MonthlyRefillCount BIGINT,&#xA;TotalRefillCount BIGINT&#xA;)&#xA;&#xA;INSERT INTO #EligiblePatients&#xA;SELECT SUM(DailyReferralCount),&#xA; SUM(WeeklyReferralCount),&#xA; SUM(MonthlyReferralCount),&#xA; COUNT(DISTINCT MedicalRecordNo),&#xA; SUM(DailyRefillCount),&#xA; SUM(DailyOrderMadeReadyToFillCount),&#xA; SUM(WeeklyRefillCount),&#xA; SUM(MonthlyRefillCount),&#xA; SUM(TotalRefillCount) + 214&#xA;&#xA;FROM (&#xA;SELECT DISTINCT Patient.MedicalRecordNo,CAST(Patient.ReferralDate AS DATE) AS ReferralDate,ReferralSource.OrganizationName as ReferralSource&#xA;,CASE WHEN cast(ReferralDate as date) = CAST(GETDATE() AS DATE) THEN 1 ELSE 0 END AS DailyReferralCount&#xA;,CASE WHEN DATEPART(WEEK,ReferralDate) = DATEPART(WEEK,GETDATE()) AND YEAR(ReferralDate) = YEAR(GETDATE()) THEN 1 ELSE 0 END AS WeeklyReferralCount&#xA;,CASE WHEN  MONTH(ReferralDate) = MONTH(GETDATE()) AND YEAR(ReferralDate) = YEAR(GETDATE())  THEN 1 ELSE 0 END AS MonthlyReferralCount&#xA;,CASE WHEN cast(ActualCompoundingDate as date) = CAST(GETDATE() AS DATE) THEN 1 ELSE 0 END AS DailyRefillCount&#xA;,CASE WHEN CAST(PrescriptionFill.CreatedDate AS DATE) = CAST(GETDATE() AS DATE) THEN 1 ELSE 0 END AS DailyOrderMadeReadyToFillCount&#xA;,CASE WHEN DATEPART(WEEK,ActualCompoundingDate) = DATEPART(WEEK,GETDATE()) AND YEAR(ActualCompoundingDate) = YEAR(GETDATE()) THEN 1 ELSE 0 END AS WeeklyRefillCount&#xA;,CASE WHEN  MONTH(ActualCompoundingDate) = MONTH(GETDATE()) AND YEAR(ActualCompoundingDate) = YEAR(GETDATE())  THEN 1 ELSE 0 END AS MonthlyRefillCount&#xA;,CASE WHEN PrescriptionFill.Id is not null  THEN 1 ELSE 0 END AS TotalRefillCount&#xA; FROM PATIENT.PATIENT&#xA;INNER JOIN Common.Person ReferralSource on ReferralSource.Id = Patient.ReferralSource_Id&#xA;left JOIN Prescription.PrescRiption on Prescription.Patient_Id = Patient.Id&#xA;left JOIN Prescription.PrescriptionFill on PrescriptionFill.Prescription_Id = Prescription.Id&#xA;AND PrescriptionFill.Sequence != 1&#xA;WHERE Patient.Team_Id = 500043&#xA;) A&#xA;&#xA;SELECT * FROM #EligiblePatients&#xA;&#xA;DROP TABLE #EligiblePatients"
